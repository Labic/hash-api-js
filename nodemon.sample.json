{
  "restartable": "rs",
  "ignore": [
    ".git",
    "node_modules"
  ],
  "verbose": true,
  "execMap": {
    "node": "npm start"
  },
  "events": {
    "restart": "osascript -e 'display notification \"App restarted due to:\n'$FILENAME'\" with title \"nodemon\"'"
  },
  "watch": [
    "*.*"
  ],
  "env": {
    "CACHE_ENABLED": false,
    "DEBUG":"hashapi*",
    "ES_URI_0": "localhost:9200",
    "HOST": "localhost",
    "HTTP": false,
    "LEGACY_EXPLORER": false,
    "LIVE_RELOAD": false,
    "MONGODB_URI": "",
    "NODE_ENV": "development",
    "PORT": 3000,
    "REST_API_ROOT": "/v2",
    "SENTRY_DNS": "https://user:pass@sentry.io/id"
  },
  "ext": "js json"
}