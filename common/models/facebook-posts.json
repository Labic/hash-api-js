{
  "name": "FacebookPost",
  "plural": "facebook_posts",
  "base": "PersistedModel",
  "strict": true,
  "http": {"path": "facebook/posts"},
  "options": {
    "mongodb": {
      "collection": "facebook_posts"
    }
  },
  "properties": {
    "id": {
      "type": "Number",
      "id": true
    },
    "from": {
      "type": "FacebookPostFrom"
    },
    "sources": {
      "type": "FacebookPostSources"
    },
    "url": {
      "type": "String"
    }
  },
  "scope": {
    "limit": 25
  }
}
