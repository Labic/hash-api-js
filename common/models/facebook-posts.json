      "collection": "facebook_posts"
    }
  },
  "properties": {
    "id": {
      "type": "Number",
      "id": true
    },
    "from": {
      "type": "Object"
    },
    "created_time": {
      "type": "Date"
    },
    "message": {
      "type": "String"
    },
    "name": {
      "type": "String"
    },
    "link": {
      "type": "String"
    },
    "scraper": {
      "type": "Object"
    },
    "url": {
      "type": "String"
    }
  },
  "scope": {
    "limit": 25
  }
}
