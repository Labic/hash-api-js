//- ========================================================================================
//- Progress Bar ===========================================================================
//- ========================================================================================
mixin widgetProgressBar(title, color, value, symbol, position, progress_bar, value_difference, period)
	.col-md-3.col-sm-6.m-b-10
		.tiles(class=color)
			.tiles-body
				.controller
					a.reload(href='#')
					a.remove(href='#')
				.tiles-title= title
					.heading
						if position == 'before'
							| #{symbol}
							span.animate-number(data-value=value, data-animation-duration='500')
								| #{value}
						else
							span.animate-number(data-value=value, data-animation-duration='500')
								| #{value}
							| #{symbol}
				.progress.transparent.progress-small.no-radius
					.progress-bar.progress-bar-white.animate-progress-bar(data-percentage=progress_bar + '%', style='width: ' + progress_bar + '%;')
				.description
					i.icon-custom-up
					span.text-white.mini-description= value_difference + '% higher'
						span.blend= period

//- ========================================================================================
//- Tweets Updates =========================================================================
//- ========================================================================================
mixin widgetTweetsUpdates(options, tweets)
	.row.tiles-container
		.col-md-12
			.m-l-10
				.tiles.grey.p-t-10.p-b-10.p-l-25
					h5.text-black.semi-bold
						| #{options.title}
				.scrollbar.tiles.white(data-uri='#{tweets.url}', data-skip='10', data-loading='false')
					if tweets
						each tweet in tweets
							.p-t-10.p-b-5.b-b.b-grey
								.post
									.user-profile-pic-wrapper
										.user-profile-pic-2x.white-border
											a(href='https://twitter.com/#{tweet.status.user.screen_name}', 
											  target='_blank')
												img(width='45', height='45', src='#{tweet.status.user.profile_image_url_https}', alt='#{tweet.status.user.screen_name}')
									.info-wrapper.inline
										.info.text-black
											strong.user-info
												a(href='https://twitter.com/#{tweet.status.user.screen_name}', target='_blank')
													| @#{tweet.status.user.screen_name} 
											| - 
											span.user-followers
												| #{tweet.status.user.followers_count} Seguidores
											p.text
												| #{tweet.status.text.replace(/\n/g, '<br />')}
											a.muted.small-text(href="https://twitter.com/#{tweet.status.user.screen_name}/status/#{tweet.status.id_str}", target="_blank")
												|  #{moment(tweet.status.created_at).fromNow()} | #{tweet.rts} Retweets 
											//-a(href='#')
												i.fa.fa-star-o.pull-right
									//-	.clearfix
									//-.inline.pull-right
									//-	.tiles.text-white.p-t-5.p-l-5.p-b-5.p-r-5.inline
									//-		i.fa.fa-heart-o.fa-lg
									//-	.tiles.white.p-t-5.p-l-5.p-b-5.p-r-5.inline
									//-		i.fa.fa-comment-o.fa-lg
									.clearfix
					else
						//- TODO: Fazer tradução
						.p-t-10.p-b-5.b-b.b-grey.bg-danger
								| Sem Tweets a serem exibidos. :-(
//- ========================================================================================
//- Facebook Posts =========================================================================
//- ========================================================================================
mixin widgetFacebookPosts(options, facebookSections)
	.row.tiles-container
		.col-md-12
			.m-l-10
				.tiles.grey.p-t-10.p-b-10.p-l-25
					h5.text-black.semi-bold
						| #{options.title}
				.scrollbar.tiles.white(data-uri='#{facebookSections.url}', data-skip='10', data-loading='false')
					if facebookSections
						each facebookSection in facebookSections
							.p-t-10.p-b-5.b-b.b-grey
								.post
									.user-profile-pic-wrapper
										.user-profile-pic-2x.white-border
											a(href='#')
												img(width='45', height='45', src='#{facebookSection.Profile}', alt='#{facebookSection.UserName}')
									.info-wrapper.inline
										.info.text-black
											strong
												| #{facebookSection.UserName}
											if facebookSection.Caption
												p.readmore
													| #{facebookSection.Caption.replace(/\n/g, '<br />')}
											if facebookSection.Description
												p.readmore
													| #{facebookSection.Description.replace(/\n/g, '<br />')}
											if facebookSection.Message
												p.readmore
													| #{facebookSection.Message.replace(/\n/g, '<br />')}
											span.muted.small-text
												|  a #{moment.unix(facebookSection.CreatedTime).fromNow()} | #{facebookSection.LikesCount} Likes | #{facebookSection.CommentsCount} Coment&aacute;rios | #{facebookSection.SharesCount} Compartilhamentos
											//-a(href='#')
												i.fa.fa-star-o.pull-right
									//-	.clearfix
									//-.inline.pull-right
									//-	.tiles.text-white.p-t-5.p-l-5.p-b-5.p-r-5.inline
									//-		i.fa.fa-heart-o.fa-lg
									//-	.tiles.white.p-t-5.p-l-5.p-b-5.p-r-5.inline
									//-		i.fa.fa-comment-o.fa-lg
									.clearfix
					else
						//- TODO: Fazer tradução
						.p-t-10.p-b-5.b-b.b-grey.bg-danger
								| Sem Posts a serem exibidos. :-(

