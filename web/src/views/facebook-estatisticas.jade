doctype html
html
	include head.jade
		block pluginCSS
			link(rel="stylesheet", type="text/css", href="/assets/plugins/fancybox/jquery.fancybox.min.css")
	
	// BEGIN BODY
	body.no-top.hide-top-content-header
		include header.jade
		
		// BEGIN CONTAINER
		#twitter.page-container
			include sidebar.jade
			
			// BEGIN PAGE CONTAINER

			.page-content
				.content.sm-gutter
					include widgets.jade
					.row
						.col-md-6
							h2
								| Seção 2
							a.fancybox(href='http://104.131.228.31/facebook/secao_2_face.jpeg?timestamp={moment().format("X")}')
								img(src='http://104.131.228.31/facebook/secao_2_face.jpeg?timestamp={moment().format("X")}', width='100%')
						.col-md-6
							h2
								| Seção 3
							a.fancybox(href='http://104.131.228.31/facebook/secao_3_face.jpeg?timestamp={moment().format("X")}')
								img(src='http://104.131.228.31/facebook/secao_3_face.jpeg?timestamp={moment().format("X")}', width='100%')
						.col-md-6
							h2
								| Nuvem de palavras Facebook
							img(src='http://104.131.228.31/facebook/nuvem_enem_face.png?timestamp={moment().format("X")}', width='100%')

			// END PAGE CONTAINER
			
		include footer.jade
			block pageScripts
				script(src='/assets/plugins/fancybox/jquery.fancybox.pack.min.js')