{
  "name": "FacebookPagePost",
  "plural": "facebook_page_posts",
  "base": "FacebookPost",
  "strict": true,
  "http": {"path": "facebook/page/posts"},
  "options": {
    "mongodb": {
      "collection": "facebook_pages"
    }
  },
  "properties": {
    "id": {
      "id": true,
      "type": "string",
      "generated": false,
      "required": true
    }
  },
  "mixins": {
      "Mongodb": null
  },
 "relations": {
    "comments": {
      "type": "hasMany",
      "model": "FacebookComment",
      "foreignKey": "post_id"
    }
  }
}
