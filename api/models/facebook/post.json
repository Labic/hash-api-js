{
  "name": "FacebookPost",
  "base": "PersistedModel",
  "strict": true,
  "http": {"path": "facebook/posts"},
  "options": {
    "mongodb": {
      "collection": "face_pages"
    }
  },
  "properties": {
    "id": {
      "id": true,
      "type": "string",
      "generated": false
    },
    "from": {
      "type": "FacebookProfile"
    },
    "url": {
      "type": "string"
    },
    "caption": {
      "type": "string"
    },
    "description": {
      "type": "string"
    },
    "message": {
      "type": "string"
    },
    "hashtags": {
      "type": ["string"]
    },
    "name": {
      "type": "string"
    },
    "object_id": {
      "type": "number"
    },
    "to": {
      "type": "FacebookProfile"
    },
    "comments_count": {
      "type": "number"
    },
    "likes_count": {
      "type": "number"
    },
    "full_picture": {
      "type": "string"
    },
    "type": {
      "type": "string"
    },
    "story": {
      "type": "string"
    },
    "status_type": {
      "type": "string"
    },
    "shares_count": {
      "type": "number"
    },
    "link": {
      "type": "object"
    },
    "picture": {
      "type": "object"
    },
    "properties": {
      "type": ["object"]
    },
    "with_tags": {
      "type": ["FacebookProfile"]
    },
    "message_tags": {
      "type": ["FacebookProfile"]
    },
    "scheduled_publish_time_ms": {
      "type": "number"
    },
    "updated_time": {
      "type": "date"
    },
    "updated_time_ms": {
      "type": "number"
    },
    "created_time": {
      "type": "date"
    },
    "created_time_ms": {
      "type": "number"
    },
    "api_inf": {
      "type": "object"
    },
    "collect_at": {
      "type": "number"
    }
  },
  "mixins": {
    "Mongodb": null,
    "DisableMethods": {
      "selfMethods": [
        "replaceOrCreate",
        "upsertWithWhere",
        "findOne",
        "replaceById",
        "count",
        "upsert",
        "findOrCreate",
        "exists",
        "deleteById",
        "updateAll",
        "updateAttributes",
        "createChangeStream",
        "delete_comments",
        "destroyById_comments"
      ]
    }
  },
 "relations": {
    "comments": {
      "type": "hasMany",
      "model": "FacebookComment",
      "foreignKey": "post_id"
    }
  },
  "scope": {
    "limit": 25
  }
}
