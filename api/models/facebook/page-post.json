{
  "name": "FacebookPagePost",
  "plural": "facebook_page_posts",
  "base": "FacebookPost",
  "strict": true,
  "http": {"path": "facebook/page/posts"},
  "options": {
    "mongodb": {
      "collection": "face_pages"
    }
  },
  "mixins": {
    "Mongodb": null,
    "DisableMethods": {
      "selfMethods": [
        "count",
        "upsert",
        "findOne",
        "findOrCreate",
        "deleteById",
        "updateAll",
        "updateAttributes",
        "createChangeStream"
      ]
    }
  },
 "relations": {
    "comments": {
      "type": "hasMany",
      "model": "FacebookComment",
      "foreignKey": "post_id"
    }
  }
}
