{
  "name": "InstagramMedia",
  "plural": "medias",
  "base": "PersistedModel",
  "http": {"path": "instagram/medias"},
  "options": {
    "mongodb": {
      "collection": "instagram_media"
    }
  },
  "properties": {
    "id": {
      "type": "string",
      "id": true
    },
    "data": {
      "type": "InstagramMediaData"
    },
    "categories": {
      "type": ["string"]
    }
  },
  "mixins": {
    "Mongodb": null,
    "DisableMethods": {
      "selfMethods": [
        "count",
        "upsert",
        "findOne",
        "findOrCreate",
        "deleteById",
        "updateAll",
        "updateAttributes",
        "createChangeStream"
      ]
    }
  },
  "scope": {
    "limit": 25
  },
  "acls": [
    {
      "accessType": "WRITE",
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "DENY"
    },
    {
      "accessType": "READ",
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "ALLOW"
    }
  ]
}
